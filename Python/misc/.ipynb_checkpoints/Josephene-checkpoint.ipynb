   "source": [
    "from collections import deque\n",
    "\n",
    "soldiers = deque([*range(1,8)])\n",
    "\n",
    "while len(soldiers) > 1:\n",
    "    soldiers.rotate(-2)\n",
    "    print(soldiers.pop())\n",
    "print('survivor:' , soldiers.pop())"
   ]
